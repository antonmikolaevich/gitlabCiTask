{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-undef":"off",
        "no-unused-vars":"warn",
        "no-useless-escape":"off",
        "no-useless-catch":"off",
        "no-mixed-spaces-and-tabs":"off",
        "no-var": "off",
        "max-len": "off",
        "require-jsdoc": "off",
        "no-invalid-this": "off",
        "camelcase": "off",
        "prefer-rest-params": "off",
        "new-cap": "off",
        "prefer-spread": "off",
        "guard-for-in": "off",
        "valid-jsdoc": "off",
        "no-tabs": "off"
    }
}
